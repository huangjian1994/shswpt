<struts>

	<package name="ssu" namespace="/ssu" extends="struts-default">
		<action name="ssu_*" class="SsuAction" method="{1}" >
		<result name="t" >/jsp/lb.jsp</result>
		
		</action>
	</package>
		
	<package name="soa" namespace="/soa" extends="struts-default">
		<action name="soa_*" class="SoaXXXAction" method="{1}" >
		<result name="login" >/jsp/test.jsp</result>
		<result name="error" >/jsp/error.jsp</result>
		<result name="index" >/jsp/welcome.jsp</result>
		</action>
	</package>
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
